# === Core switch ===
# openai | scads
LLM_PROVIDER=openai

# === OpenAI ===
# 获取后填真实值到 .env（不要提交到仓库）
OPENAI_API_KEY=__REPLACE_WITH_YOUR_OPENAI_API_KEY__
OPENAI_MODEL=gpt-4.1-mini
# 如使用代理或自托管网关才需要
# OPENAI_BASE_URL=https://api.openai.com/v1

# === SCADS (只在 LLM_PROVIDER=scads 时使用) ===
SCADS_API_KEY=__REPLACE_WITH_YOUR_SCADS_API_KEY__
SCADS_MODEL=meta-llama/Llama-3.3-70B-Instruct
SCADS_BASE_URL=https://llm.scads.ai/v1
